.ad image{
	width: 75rpx;
	height: 72rpx;
	margin-bottom: 15rpx;
}

/* 家空间 */
.space{
	background-color: white;
	padding: 20rpx 30rpx;
	-webkit-box-sizing: border-box;
	        box-sizing: border-box;
}
.house{
	padding: 30rpx 0;
	-webkit-box-sizing: border-box;
	        box-sizing: border-box;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	-webkit-box-shadow: 0 0 5rpx 5rpx rgba(0,0,0,0.03);
	        box-shadow: 0 0 5rpx 5rpx rgba(0,0,0,0.03);
	border-radius: 20rpx;
}
.house view{
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-orient: vertical;
	-webkit-box-direction: normal;
	-webkit-flex-direction: column;
	        flex-direction: column;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	font-size: 25rpx;
	width: 33.3%;
	border-right: 1rpx solid #F2F2F2;
}
.house view:last-child{
	border-right: none;
}
.tit{
	font-size: 33rpx;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	font-weight: bold;
	-webkit-box-pack: justify;
	-webkit-justify-content: space-between;
	        justify-content: space-between;
}
.more{
	width: 15rpx;
	height: 20rpx;
}
.check{
	font-size: 24rpx;
	color: #a8a8a8;
	font-weight: normal;
	margin-right: 10rpx;
}
.type{
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	margin-top: 33rpx;
}
.type image{
	width: 50%;
	height: 130rpx;
}
.room image{
	width: 100%;
	height: 140rpx;
	margin-bottom: 10rpx;
}
.room .bor{
	width: 50%;
	margin: 10px 0;
}
.room .bor:nth-child(2n-1){
	border-right: 1px solid #F2F2F2;
}
.room view{
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-justify-content: space-around;
	        justify-content: space-around;
	height: 100%;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	-webkit-flex-wrap: wrap;
	        flex-wrap: wrap;
}
.design-ad{
	width: 100%;
	height: 140rpx;
	display: block;
}

/* 现货推荐 */
.now{
	padding: 0 30rpx;
	-webkit-box-sizing: border-box;
	        box-sizing: border-box;
}
.swiper2{
	width: 100%;
	margin-bottom: 20rpx;
}
.swiper2 image{
	height: 225rpx;
	width: 90%;
	display: block;
	margin: auto;
}
.swiper2 view{
