/* 商品列表 */
.product{
	padding: 20rpx 30rpx;
	-webkit-box-sizing: border-box;
	        box-sizing: border-box;
	margin-top: 15rpx;
	background-color: white;
}
.title{
	padding: 0 20rpx;
	-webkit-box-sizing: border-box;
	        box-sizing: border-box;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	-webkit-box-pack: justify;
	-webkit-justify-content: space-between;
	        justify-content: space-between;
}
.topic{
	font-size: 35rpx;
	font-weight: 600;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
}
.topic .a{
	position: relative;
	font-weight: bold;
}
.topic .a::before{
	content: '';
	width: 55rpx;
	height: 6rpx;
	background-color: #40CCCC;
	position: absolute;
	top: 50%;
	left: -65rpx;
	-webkit-transform: translateY(-50%);
	        transform: translateY(-50%);
}
.topic text:nth-child(3){
	font-size: 30rpx;
	font-weight: normal;
}
.num{
	font-size: 25rpx;
	font-weight: bold;
}
.num text{
	font-size: 20rpx;
	font-weight: normal;
}

/* 专题轮播 */
.swiper1{
	width: 100%;
	margin-bottom: 20rpx;
}
.swiper1 image{
	height: 245rpx;
	width: 100%;
	border-radius: 30rpx 30rpx 0 0;
}
.swiper1 view{
	-webkit-box-shadow: 0 0 5rpx 5rpx rgba(0,0,0,0.03);
	        box-shadow: 0 0 5rpx 5rpx rgba(0,0,0,0.03);
	border-radius: 30rpx;
	padding-bottom: 20rpx;
	-webkit-box-sizing: border-box;
	        box-sizing: border-box;
}
.swiper1 swiper-item{
	font-size: 30rpx;
	margin-top: 50rpx;
}
.swiper1 .subject{
	display: block;
	font-size: 30rpx;
	margin: 30rpx 20rpx 10rpx;
	color: #030303;
	font-weight: 600;
}
.swiper1 .intro{
	font-size: 20rpx;
	color: #666666;
	width:100%;
	text-overflow: -o-ellipsis-lastline; 
	overflow: hidden;
	text-overflow: ellipsis; 
	display: -webkit-box;
	-webkit-line-clamp: 1;
	line-clamp: 1; 
	-webkit-box-orient: vertical;
	white-space: normal;
	padding: 0 20rpx;
	-webkit-box-sizing: border-box;
	        box-sizing: border-box;
}
.list-nav{
	width: 100%;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-justify-content: space-around;
	        justify-content: space-around;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	font-size: 24rpx;
	color: #999999;
	padding: 30rpx 0;
	-webkit-box-shadow: 0 0 5rpx 3rpx rgba(0,0,0,0.1);
	        box-shadow: 0 0 5rpx 3rpx rgba(0,0,0,0.1);
}
.navactive{
	color: #40CCCB;
	font-size: 30rpx;
	font-weight: bold;
	position: relative;
}
.navactive::after{
	content: '';
	width: 100%;
	height: 5rpx;
	background-color: #40CCCB;
	position: absolute;
	left: 0;
	bottom: -30rpx;
}
.list-detail{
	padding: 0 30rpx 20rpx;
