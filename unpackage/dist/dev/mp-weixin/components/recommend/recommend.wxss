	-webkit-box-pack: justify;
	-webkit-justify-content: space-between;
	        justify-content: space-between;
	-webkit-flex-wrap: wrap;
	        flex-wrap: wrap;
}
.list-detail view{
	width: 48%;
	-webkit-box-shadow: 0 0 5rpx 3rpx rgba(0,0,0,0.03);
	        box-shadow: 0 0 5rpx 3rpx rgba(0,0,0,0.03);
