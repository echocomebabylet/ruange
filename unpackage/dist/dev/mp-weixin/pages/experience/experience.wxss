/* 推荐轮播 */
.slide-image {
  position: absolute;
  height: 340rpx;
  width: 570rpx;
  border-radius: 15rpx;
  z-index: 5;
  opacity: 0.7;
  top: 16%;
  margin: 0 40rpx;
}
swiper {
  height: 430rpx;
}
.active {
  opacity: 1;
  z-index: 10;
  height: 380rpx;
  width: 572rpx;
  top: 12%;
  -webkit-transition: all 0.2s ease-in 0s;
  transition: all 0.2s ease-in 0s;
}
