
page{
	background-color: #F2F2F2;
	padding-top: 130rpx;
	-webkit-box-sizing: border-box;
	        box-sizing: border-box;
}
.nav{
	padding: 0 45rpx;
	-webkit-box-sizing: border-box;
	        box-sizing: border-box;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-pack: justify;
	-webkit-justify-content: space-between;
	        justify-content: space-between;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	font-size: 30rpx;
	height: 130rpx;
	width: 100%;
	position: fixed;
	top: 0;
	left: 0;
	z-index: 1000;
	background-color: white;
}
.nav image{
	width: 24rpx;
	height: 38rpx;
}
.active{
	position: relative;
	color: #000000;
}
.active::before{
	content: '';
	width: 100%;
	height: 5rpx;
	background-color: #40CCCB;
	position: absolute;
	bottom: -20rpx;
	left: 0;
}
.a{
	margin-left: 60rpx;
}

