
.search{
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	padding: 45rpx 45rpx 25rpx;
	-webkit-box-sizing: border-box;
	        box-sizing: border-box;
	border-bottom: 2rpx solid #F7F7F7;
	background-color: white;
}
.search image{
	width: 45rpx;
	height: 45rpx;
}
.search input{
	width: 80%;
	font-size: 28rpx;
	margin-left: 10rpx;
	padding-left: 20rpx;
	height: 54rpx;
	-webkit-box-sizing: border-box;
	        box-sizing: border-box;
	border-bottom: 1rpx solid #F7F7F7;
}
.search text{
	font-size: 28rpx;
	margin-left: 20rpx;
}
.both{
	padding: 25rpx 20rpx;
	-webkit-box-sizing: border-box;
	        box-sizing: border-box;
	font-size: 24rpx;
}
.history{
	padding: 25rpx 20rpx;
	-webkit-box-sizing: border-box;
	        box-sizing: border-box;
	font-size: 24rpx;
}
.history view{
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-pack: justify;
	-webkit-justify-content: space-between;
	        justify-content: space-between;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	padding: 25rpx 0;
	-webkit-box-sizing: border-box;
	        box-sizing: border-box;
	border-bottom: 1rpx solid #F7F7F7;
}
.history image{
	width: 25rpx;
	height: 25rpx;
}

