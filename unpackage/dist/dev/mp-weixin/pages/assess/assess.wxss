
page {
	background-color: #F2F2F2;
	padding-top: 120rpx;
	-webkit-box-sizing: border-box;
	        box-sizing: border-box;
}
.nav {
	width: 100%;
	height: 120rpx;
	background-color: white;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-pack: justify;
	-webkit-justify-content: space-between;
	        justify-content: space-between;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	padding: 0 30rpx;
	-webkit-box-sizing: border-box;
	        box-sizing: border-box;
	position: fixed;
	top: 0;
	left: 0;
	font-size: 30rpx;
	z-index: 100;
}
.nav image {
	width: 50rpx;
	height: 50rpx;
}
.assess-list{
	padding: 30rpx;
	-webkit-box-sizing: border-box;
	        box-sizing: border-box;
	border-top: 1rpx solid #F7F7F7;
	background-color: white;
}
.user{
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-pack: justify;
	-webkit-justify-content: space-between;
	        justify-content: space-between;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
}
.user view{
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	font-size: 24rpx;
}
.user .head{
	width: 50rpx;
	height: 50rpx;
	border-radius: 50%;
	margin-right: 10rpx;
}
.user .star image{
	width: 24rpx;
	height: 24rpx;
	margin-left: 10rpx;
}
.content{
	width: 100%;
	font-size: 24rpx;
	margin: 30rpx 0;
}
.assessimg{
	width: 380rpx;
	margin-left: 90rpx;
}
.assessimg image{
	width: 49%;
	height: 180rpx;
}
.assessimg image:nth-child(2n-1){
	margin-right: 5rpx;
}

