	-webkit-justify-content: space-between;
	        justify-content: space-between;
	font-size: 25rpx;
	color: #ababab;
	position: fixed;
	top: 130rpx;
	left: 0;
	z-index: 1000;
	background-color: white;
	border-bottom: 1rpx solid #F7F7F7;
}
.active{
	color: #40CCCB;
	position: relative;
}
.active::before{
	content: '';
	width: 100%;
	height: 5rpx;
	background-color: #40CCCB;
	position: absolute;
	bottom: -30rpx;
	left: 0;
}
