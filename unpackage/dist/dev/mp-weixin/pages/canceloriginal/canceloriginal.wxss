	width: 24rpx;
	height: 38rpx;
}
.list{
	margin-top: 20rpx;
	background-color: white;
	padding: 0 30rpx;
	-webkit-box-sizing: border-box;
	        box-sizing: border-box;
}
.list view{
	border-bottom: 1rpx solid #F7F7F7;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
