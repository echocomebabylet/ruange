/* 购物券隐藏 */
.coupon-list{
	width: 100%;
	height: 100%;
	background-color: rgba(0,0,0,0.5);
	position: fixed;
	top: 0;
	left: 0;
	z-index: 200;
}
.cont{
	width: 100%;
	padding: 0 30rpx;
	position: absolute;
	bottom: 0;
	left: 0;
	border-radius: 15rpx 15rpx 0 0;
	-webkit-box-sizing: border-box;
	        box-sizing: border-box;
	background-color: white;
}
.coupon-nav{
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-pack: justify;
	-webkit-justify-content: space-between;
	        justify-content: space-between;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	font-size: 32rpx;
	padding: 40rpx 0;
	-webkit-box-sizing: border-box;
	        box-sizing: border-box;
}
.coupon-nav image{
	width: 24rpx;
	height: 24rpx;
}
.coupon-select{
	padding: 30rpx 200rpx;
	-webkit-box-sizing: border-box;
	        box-sizing: border-box;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-pack: justify;
	-webkit-justify-content: space-between;
	        justify-content: space-between;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	font-size: 30rpx;
}
.coupon-select .cactive{
	position: relative;
	color: #40CCCB;
}
.coupon-select .cactive::after{
	content: '';
	width: 50rpx;
	height: 6rpx;
	background-color: #40CCCB;
	position: absolute;
	bottom: -30rpx;
	left: 50%;
	-webkit-transform: translateX(-50%);
	        transform: translateX(-50%);
}
scroll-view{
	margin-top: 40rpx;
	height: 800rpx;
	padding-bottom: 110rpx;
	-webkit-box-sizing: border-box;
	        box-sizing: border-box;
}
/* 满减 */
.reduction{
	width: 30%;
	height: 208rpx;
	position: relative;
}
.reduction image{
	width: 100%;
	height: 100%;
}
.reduction view{
	width: 100%;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	color: white;
	font-size: 34rpx;
	-webkit-box-orient: vertical;
	-webkit-box-direction: normal;
	-webkit-flex-direction: column;
	        flex-direction: column;
	-webkit-flex-wrap: nowrap;
	        flex-wrap: nowrap;
	position: absolute;
	top: 50%;
	left: 50%;
	-webkit-transform: translateX(-50%) translateY(-50%);
	        transform: translateX(-50%) translateY(-50%);
}
