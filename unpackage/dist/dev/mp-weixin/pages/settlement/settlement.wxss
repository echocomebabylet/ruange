
page{
	padding-top: 150rpx;
	padding-bottom: 120rpx;
	background-color: #F2F2F2;
	-webkit-box-sizing: border-box;
	        box-sizing: border-box;
}
.nav{
	padding: 0 35rpx;
	-webkit-box-sizing: border-box;
	        box-sizing: border-box;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-pack: justify;
	-webkit-justify-content: space-between;
	        justify-content: space-between;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	font-size: 30rpx;
	height: 130rpx;
	background-color: white;
	width: 100%;
	position: fixed;
	top: 0;
	left: 0;
	z-index: 1000;
}
.nav image{
	width: 24rpx;
	height: 38rpx;
}
.list{
	background-color: white;
	padding: 0 35rpx;
	-webkit-box-sizing: border-box;
	        box-sizing: border-box;
	font-size: 28rpx;
	overflow: hidden;
}
.photo{
	width: 215rpx;
	height: 215rpx;
	border-radius: 50%;
	background-color: #40CCCB;
	margin: 60rpx auto 30rpx;
	overflow: hidden;
}
.photo image{
	width: 100rpx;
	height: 95rpx;
	display: block;
}
.intro{
	width: 100%;
	padding: 20rpx 10rpx;
	border: 1rpx solid #F7F7F7;
	color: #b2b2b2;
	margin-bottom: 40rpx;
}
.upload{
	width: 540rpx;
	height: 380rpx;
	background-color: #e6e4e5;
	margin: 40rpx auto;
	overflow: hidden;
	text-align: center;
}
.upload image{
	width: 70rpx;
	height: 70rpx;
	display: block;
	margin: 110rpx auto 30rpx;
}
.submit{
	width: 100%;
	height: 120rpx;
	line-height: 120rpx;
	text-align: center;
	font-size: 30rpx;
	background-color: #40CCCB;
	color: white;
	position: fixed;
	bottom: 0;
	left: 0;
	z-index: 1000;
}

