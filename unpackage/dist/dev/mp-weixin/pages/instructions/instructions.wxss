
.nav{
	background-color: white;
	font-size: 30rpx;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	-webkit-box-sizing: border-box;
	        box-sizing: border-box;
	padding: 40rpx;
	border-bottom: 1rpx solid #F7F7F7;
}
