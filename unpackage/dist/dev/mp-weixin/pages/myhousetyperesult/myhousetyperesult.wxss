
page{
	padding-top: 240rpx;
	-webkit-box-sizing: border-box;
	        box-sizing: border-box;
	background-color: #F2F2F2;
}
.nav{
	padding: 0 45rpx;
	-webkit-box-sizing: border-box;
	        box-sizing: border-box;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-pack: justify;
	-webkit-justify-content: space-between;
	        justify-content: space-between;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	font-size: 32rpx;
	font-weight: bold;
	height: 130rpx;
	background-color: white;
	width: 100%;
	position: fixed;
	top: 0;
	left: 0;
	z-index: 1000;
}
.nav image{
	width: 24rpx;
	height: 38rpx;
}
.list{
	background-color: white;
	padding: 60rpx;
	-webkit-box-sizing: border-box;
	        box-sizing: border-box;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	margin-bottom: 20rpx;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	font-size: 26rpx;
}
.list image{
	width: 270rpx;
	height: 240rpx;
	margin-right: 40rpx;
}
.list view view{
	width: 180rpx;
	height: 50rpx;
	line-height: 50rpx;
	text-align: center;
	font-size: 22rpx;
	border-radius: 5rpx;
	margin-top: 40rpx;
	-webkit-align-self: flex-end;
	        align-self: flex-end;
	border: 1rpx solid #F7F7F7;
}
.active{
	width: 180rpx;
	height: 50rpx;
	line-height: 50rpx;
	text-align: center;
	font-size: 22rpx;
	border-radius: 5rpx;
	margin-top: 40rpx;
	-webkit-align-self: flex-end;
	        align-self: flex-end;
	background-color: #40CCCB;
	color: white;
	border: none !important;
}
.search{
	padding: 30rpx;
	-webkit-box-sizing: border-box;
	        box-sizing: border-box;
	background-color: white;
	width: 100%;
	position: fixed;
	top: 130rpx;
	left: 0;
	z-index: 1000;
}
.search view{
	width: 100%;
	height: 66rpx;
	background-color: #f5f5f5;
	border-radius: 40rpx;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	padding: 0 20rpx;
}

