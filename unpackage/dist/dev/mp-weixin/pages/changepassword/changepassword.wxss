
.nav{
	padding: 0 30rpx;
	-webkit-box-sizing: border-box;
	        box-sizing: border-box;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-pack: justify;
	-webkit-justify-content: space-between;
	        justify-content: space-between;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	font-size: 30rpx;
	height: 130rpx;
	background-color: white;
	border-bottom: 1rpx solid #F7F7F7;
}
.nav image{
	width: 24rpx;
	height: 38rpx;
}
.tel{
	width: 80%;
	border-bottom: 2rpx solid #40CCCB;
	font-size: 24rpx;
	padding: 35rpx 10rpx;
	-webkit-box-sizing: border-box;
	        box-sizing: border-box;
	margin: auto;
	margin-top: 200rpx;
}
.code{
	width: 50%;
	border-bottom: 2rpx solid #40CCCB;
	font-size: 24rpx;
	padding: 35rpx 10rpx;
	-webkit-box-sizing: border-box;
	        box-sizing: border-box;
}
.send{
	width: 200rpx;
	height: 70rpx;
	line-height: 70rpx;
	text-align: center;
	background-color: #40CCCB;
	color: white;
	font-size: 24rpx;
	border-radius: 5rpx;
	margin-left: 20rpx;
}
.btn{
	width: 80%;
	height: 90rpx;
	line-height: 90rpx;
	text-align: center;
	background-color: #40CCCB;
	color: white;
	font-size: 24rpx;
	margin: auto;
	margin-top: 100rpx;
	border-radius: 10rpx;
}

