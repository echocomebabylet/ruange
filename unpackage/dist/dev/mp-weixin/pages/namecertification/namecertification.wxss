
page{
	background-color: #f2f2f2;
}
.nav{
	padding: 0 45rpx;
	-webkit-box-sizing: border-box;
	        box-sizing: border-box;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-pack: justify;
	-webkit-justify-content: space-between;
	        justify-content: space-between;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	font-size: 32rpx;
	font-weight: bold;
	height: 130rpx;
	background-color: white;
	border-bottom: 1rpx solid #F7F7F7;
}
.nav image{
	width: 50rpx;
	height: 40rpx;
}
.btn{
	width: 80%;
	height: 100rpx;
	line-height: 100rpx;
	color: white;
	background-color: #40CCCB;
	text-align: center;
	border-radius: 50rpx;
	margin: auto;
	font-size: 30rpx;
	margin-top: 60rpx;
}
.face{
	width: 100%;
	height: 100%;
	background-color: rgba(0,0,0,0.4);
	position: fixed;
	top: 0;
	left: 0;
}
.cont{
	width: 560rpx;
	background-color: white;
	border-radius: 20rpx;
	position: absolute;
	top: 50%;
	left: 50%;
	-webkit-transform: translateX(-50%) translateY(-50%);
	        transform: translateX(-50%) translateY(-50%);
}
.yes{
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	border-top: 1rpx solid #F7F7F7;
}
.yes text{
	display: block;
	width: 50%;
	text-align: center;
	padding: 30rpx 0;
	-webkit-box-sizing: border-box;
	        box-sizing: border-box;
	font-size: 26rpx;
}

